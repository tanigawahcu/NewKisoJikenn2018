/**
 * 4-way Or: 
 * out = (in[0] or in[1] or in[2] or in[3])
 */

CHIP Or4Way {
    IN in[4];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[0],b=in[1],out=tmp[0]);
    Or(b=in[2],b=in[3],out=tmp[1]);
    Or(a=tmp[0],b=tmp[1],out=out);
}