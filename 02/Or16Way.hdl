/**
 * 16-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    Or8Way(in=in[0..7],out=tmp0);
    Or8Way(in=in[8..15],out=tmp1);
    Or(a=tmp0,b=tmp1,out=out);
}